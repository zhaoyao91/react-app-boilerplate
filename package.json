{
  "name": "ai-pja-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "start": "serve -s build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "craco test --no-watch",
    "test:watch": "craco test",
    "flow": "flow",
    "fix": "pretty-quick",
    "now-build": "npm run build"
