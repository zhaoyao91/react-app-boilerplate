    "react-dom": "^16.8.4",
    "react-i18next": "^10.5.1",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-use": "^5.14.0",
    "shortid": "^2.2.14",
    "try-to-catch": "^1.1.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^3.6.0",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "flow-bin": "^0.95.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "serve": "^10.1.2",
    "source-map-explorer": "^1.8.0"
