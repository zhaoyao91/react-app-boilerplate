  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "antd-mobile": "^2.2.9",
    "axios": "^0.18.0",
    "i18next": "^15.0.7",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "luxon": "^1.11.4",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.1.1",
    "mobx-state-tree": "^3.11.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "numbro": "^2.1.2",
    "prop-types": "^15.7.2",
